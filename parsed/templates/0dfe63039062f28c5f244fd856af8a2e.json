{"components/cs-custom/cs-navigation-megamenu":"<!-- SECOND LEVEL STARTS -->\r\n{{#if children}}\r\n<a class=\"has-subMenu needsclick\" href=\"{{url}}\">\r\n    {{name}}\r\n</a>\r\n<ul class=\"megamenu-submenu\">\r\n  <li class=\"d-flex align-start\">\r\n    <div class=\"megamenu-image-box d-flex\">\r\n      <a href=\"/categories/\">\r\n        <img src=\"/product_images/uploaded_images/nav-featured-img.jpg\" alt=\"\">\r\n      </a>\r\n    </div>\r\n    <div class=\"megamenu-list d-flex flex-wrap\">\r\n      {{#each children}}\r\n        <!-- THIRD LEVEL STARTS -->\r\n        <div class=\"megamenu-row\">\r\n          <p><a href=\"{{url}}\">{{name}}</a></p>\r\n          {{#if children}}\r\n            <div class=\"megamenu-item-wrap d-flex align-start flex-wrap\">\r\n              {{#each children}}\r\n                <!-- FORTH LEVEL STARTS -->\r\n                <div class=\"megamenu-item\">\r\n                  <a href=\"{{url}}\">{{name}}</a>\r\n                </div>\r\n                <!-- FORTH LEVEL ENDS -->\r\n              {{/each}}\r\n            </div>\r\n          {{/if}}\r\n        </div>\r\n        <!-- THIRD LEVEL ENDS -->\r\n      {{/each}}\r\n    </div>\r\n  </li>\r\n</ul>\r\n{{else}}\r\n<a href=\"{{url}}\">{{name}}</a>\r\n{{/if}}\r\n<!-- SECOND LEVEL ENDS -->\r\n"}

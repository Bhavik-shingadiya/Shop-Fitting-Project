{"components/cs-custom/cs-navigation-megamenu-new":"<!-- SECOND LEVEL STARTS -->\r\n{{#if children}}\r\n<a class=\"has-subMenu needsclick\" href=\"{{url}}\">\r\n    {{#if megatag}}\r\n      <i class=\"menu-tag\">{{megatag}}</i>\r\n    {{/if}}\r\n    {{name}}\r\n</a>\r\n<ul class=\"megamenu-submenu\">\r\n  <li class=\"container\">\r\n    {{#if megaimage}}\r\n      <div class=\"navlist-alternate-image-box float-right\">\r\n        <a href=\"{{megalink}}\">\r\n          <img src=\"/product_images/uploaded_images/{{megaimage}}\" alt=\"\"/>\r\n        </a>\r\n      </div>\r\n    {{/if}}\r\n  {{#each children}}\r\n      <p class=\"nav-cat-heading\">\r\n        <a class=\"has-subMenu needsclick\" href=\"{{url}}\">\r\n          <strong>{{name}}</strong>\r\n        </a>\r\n      </p>\r\n      {{#if children}}\r\n        {{#each children}}\r\n            <p class=\"{{#if @last}}last-child{{/if}}\"><a href=\"{{url}}\">{{name}}</a></p>\r\n        {{/each}}\r\n      {{/if}}\r\n    {{/each}}\r\n  </li>\r\n</ul>\r\n{{else}}\r\n<a class=\"has-subMenu needsclick\" href=\"{{url}}\">\r\n    {{name}}\r\n</a>\r\n{{/if}}\r\n<!-- SECOND LEVEL ENDS -->\r\n"}
